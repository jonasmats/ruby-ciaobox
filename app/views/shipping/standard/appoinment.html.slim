section.section-2.about
  .container
    figure
      = image_tag "images/green-arrow.png", class: "g-arrow", alt: "#"
    h1
      | Schedule
= render partial: "standard_step"

section.schedule-question-title.clearfix
  figure
    img alt="" src="" /
  .container.schedule-question-inner
    h2 What would you like to store?
    .month-cost
      | Monthly Estimated Cost
      span
        | $
        span.month-total-cost 0.00

section.like-to-store.clearfix
  .container
    h3 Choose how many, we'll bring them to you.
    .store-box
      .store-box-left
        a href="#" title="" 
          figure
            img alt="" src="images/bin-ic.png" /
        .store-make-space.store-box-first.store-box-normal.store-box-text
          h4 MakeSpace Bin
          span
            | $
            span.store-cost> 6.25
            | \/mo
        .store-box-text.input-copy-wrapper
          .input-copy
            .booking-icon-background-num.js-nav-min
              span.booking-icon.js-nav.icon-min
            input.item-input name="makespace" type="text" value="0" /
            .booking-icon-background-num.js-nav-plus
              span.booking-icon.js-nav.icon-plus
      .store-box-right
        a href="#" title="" 
          figure
            img alt="" src="images/wardrob-ic.png" /
        .store-box-first.store-box-normal.store-box-text
          h4 Wardrobe Box
          span
            | $
            span.store-cost> 25
            | \/mo
        .store-box-text.input-copy-wrapper
          .input-copy
            .booking-icon-background-num.js-nav-min
